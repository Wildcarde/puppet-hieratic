{
    "name": "tedivm-hieratic",
    "version": "0.5.1",
    "author": "tedivm",
    "summary": "Hieratic allows Puppet Resources to be created directly in Hiera.",
    "license": "MIT",
    "source": "https://github.com/tedivm/puppet-hieratic",
    "project_page": "https://github.com/tedivm/puppet-hieratic",
